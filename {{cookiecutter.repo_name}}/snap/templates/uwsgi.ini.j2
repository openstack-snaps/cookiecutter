# If the OpenStack service has an API that runs behind uwsgi+nginx, you'll need
# to define this template. Be sure to update the path for the wsgi-file and
# "api-name" for the socket.
[uwsgi]
wsgi-file = {{ "{{" }} snap {{ "}}" }}/bin/{{ cookiecutter.snap_name }}-wsgi-file-name
uwsgi-socket = {{ "{{" }} snap_common {{ "}}" }}/run/api-name.sock
buffer-size = 65535
master = true
enable-threads = true
processes = 4
thunder-lock = true
lazy-apps = true
home = {{ "{{" }} snap {{ "}}" }}/usr
pyargv = {{ "{{" }} pyargv {{ "}}" }}
